{
  "name": "crew-ai-ts",
  "version": "0.0.1",
  "description": "A TypeScript implementation of Crew AI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "lint": "pnpm biome lint ./src",
    "lint:fix": "pnpm biome lint --write ./src",
    "format": "pnpm biome format --write ./src",
    "check": "pnpm biome check --write ./src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "ai",
    "agents",
    "typescript",
    "crewai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.5.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.18",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.1.3",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971",
  "engines": {
    "node": ">=22.14.0"
  }
}
